<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.nki.t1.dao.BlockReasonsMapper">

    <select id="selectBlockReasons" resultType="BlockReasonsDto">
        SELECT brno AS brno,
               br_text AS brText
        FROM block_reasons
        ORDER BY brno ASC
    </select>

</mapper>